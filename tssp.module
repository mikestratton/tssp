/**
 * Implements hook_help().
 */
function tssp_help($path, $arg){
	switch($path) {
		case 'admin/help#tssp':{
			$ret_val = '<h3>' . t('About') . '</h3>';
			$ret_val .= '<p>' . t('Module created to meet specific use cases in the development of TSSP2016.') . '</p>';
			return $ret_val;
			break;
		}
	}
}